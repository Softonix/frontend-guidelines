{
  "name": "softonix-guidelines-vite",
  "version": "2.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check && \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",    
    "set-git-hooks": "cp scripts/* .git/hooks && chmod +x .git/hooks/* && git config --local core.hooksPath .git/hooks",
    "postinstall": "npm run set-git-hooks",
    "test": "npx playwright test --ui",
    "test:cy": "server-test 'dev' http://localhost:5173 'cypress open'",
    "test:cy:h": "server-test 'dev' http://localhost:5173 'cypress run'"
  },
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.16",
    "vue-router": "^4.2.5",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "element-plus": "^2.5.5",
    "portal-vue": "^3.0.0",
    "vue-i18n": "^9.9.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "cypress": "^13.6.2",
    "dotenv": "^16.3.1",
    "eslint-plugin-cypress": "^2.15.1",
    "start-server-and-test": "^2.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "npm-run-all2": "^6.1.2",
    "typescript": "~5.3.3",
    "@rushstack/eslint-patch": "^1.7.2",
    "autoprefixer": "^10.4.17",
    "eslint-config-standard": "^17.1.0",
    "pathe": "^1.1.2",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.16",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "vite": "^5.1.0",
    "vue-tsc": "^1.8.27"
  }
}