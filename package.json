{
  "name": "softonix-guidelines-vite",
  "version": "2.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "npx playwright test --ui",
    "test:cy": "start-server-and-test 'vite' http://localhost:5173 'cypress open'",
    "test:cy:h": "start-server-and-test 'vite' http://localhost:5173 'cypress run'",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --coverage --ui"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "element-plus": "^2.3.5",
    "pinia": "^2.1.3",
    "portal-vue": "^3.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0-beta.14",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@playwright/test": "^1.32.2",
    "@rushstack/eslint-patch": "^1.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.9.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "pathe": "^1.1.0",
    "postcss": "^8.4.16",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "0.15.2",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
